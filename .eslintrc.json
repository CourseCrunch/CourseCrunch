{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },"plugins": [
        "react"   
    ],
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "jsx": true
          }
    },
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "react/jsx-uses-vars": "error"
    }
}